{
  "name": "ecommerce-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev --turbopack",
    "build": "next build",
    "lint": "eslint",
    "lint-staged:eslint:fix": "eslint --fix **/*.{js,mjs,ts,tsx}",
    "lint-staged:prettier:write": "prettier --write **/*.{js,mjs,ts,tsx,css,md,json}",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "browserslist:update": "npx update-browserslist-db@latest",
    "deps:update:patch": "node scripts/update-dependencies.mjs patch",
    "deps:update:minor": "node scripts/update-dependencies.mjs minor",
    "deps:update:major": "node scripts/update-dependencies.mjs major",
    "stripe:logs": "docker logs ecommerce-store-stripe-cli"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,json}": [
      "npm run lint-staged:prettier:write"
    ],
    "*.{js,mjs,ts,tsx}": [
      "npm run lint-staged:eslint:fix"
    ]
  },
