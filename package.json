{
  "name": "ecommerce-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "next build",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint-staged:eslint:fix": "eslint --fix **/*.{js,mjs,ts,tsx}",
    "lint-staged:prettier:write": "prettier --write **/*.{js,mjs,ts,tsx,css,md,json}",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,css,md,json}": [
      "npm run lint-staged:prettier:write"
    ],
    "*.{js,mjs,ts,tsx}": [
      "npm run lint-staged:eslint:fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "3.896.0",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.544.0",
    "motion": "12.23.22",
    "next": "15.5.4",
    "prisma": "6.16.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hot-toast": "2.6.0",
    "sst": "3.9.28",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@tailwindcss/postcss": "4.1.13",
    "@types/node": "20.19.17",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
    "eslint": "9.36.0",
    "eslint-config-next": "15.3.5",
    "husky": "8.0.3",
    "lint-staged": "16.2.3",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwindcss": "4.1.13",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.2"
  }
}
